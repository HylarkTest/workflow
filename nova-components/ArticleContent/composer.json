{
    "name": "hylark/article-content",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Hylark\\ArticleContent\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hylark\\ArticleContent\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
