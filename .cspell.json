{
    "allowCompoundWords": true,
    "dictionaries": [
        "en_us",
        "softwareTerms",
        "php",
        "html",
        "css",
        "npm",
        "lorem-ipsum",
        "companies"
    ],
    "flagWords": [],
    "ignorePaths": [
        "frontend/node_modules/**",
        "frontend/hylark.babel",
        "vendor/**",
        "storage/**",
        "schema-directives.graphql",
        "clamav",
        "composer.json",
        "composer.lock",
        "frontend/package.json",
        "frontend/yarn.lock",
        "frontend/yarn-error.log",
        "bootstrap",
        "public/**",
        "database/database.sqlite",
        "database/schema.sql",
        ".phpunit.result.cache",
        ".php_cs.cache",
        "frontend/schema.json",
        "frontend/src/locales/de.json",
        "frontend/src/locales/fr.json",
        "frontend/src/locales/es.json",
        "homestead/Homestead.yaml.example",
        "homestead/user-customizations.sh",
        "phpunit.xml",
        "modules/mappings/resources/currency_symbols.json",
        "phpstan.neon.dist"
    ],
    "ignoreRegExpList": [
        "^use\\s.*$",
        "^namespace\\s.*$",
        "@param\\s[^\\s]+",
        "@(return|var|throws)\\s.*$"
    ],
    "language": "en_US",
    "overrides": [
        {
            "filename": [
                "**/en/*.php",
                "frontend/src/locales/en.json"
            ],
            "language": "en_GB"
        }
    ],
