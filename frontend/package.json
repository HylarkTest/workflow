{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vite dev",
    "prebuild": "rm -rf ../public/assets/*",
    "build": "vite build",
    "postbuild": "mv ../public/index.html ../resources/views/index.blade.php && echo \"$RELEASE\" > ../public/latest_release.txt",
    "predev": "yarn prebuild",
    "dev": "NODE_ENV=development vite build --mode=development",
    "postdev": "yarn postbuild",
    "prebuild-stage": "yarn prebuild",
    "build-stage": "NODE_ENV=staging vite build --mode=staging",
    "postbuild-stage": "yarn postbuild",
    "prebuild-templates": "rm -rf ../public/templates/*",
    "build-templates": "HYLARK_BUILD=templates vite build",
    "postbuild-templates": "mv ../public/templates-index.html ../resources/views/templates-index.blade.php",
    "prebuild-cookie-banner": "rm -rf ../public/cookie-banner/*",
    "build-cookie-banner": "HYLARK_BUILD=cookie-banner vite build",
    "postbuild-cookie-banner": "mv ../public/cookie-banner-index.html ../resources/views/cookie-banner-index.blade.php",
    "lint": "eslint --ext .js,.vue,.gql --ignore-path .gitignore src --cache",
    "lint-style": "stylelint \"src/**/*.{css,vue}\" --cache",
    "prod-lint": "NODE_ENV=production yarn lint",
    "prod-lint-style": "NODE_ENV=production yarn lint-style",
    "colors": "./scripts/build-colors-stylesheet.sh"
  },
  "dependencies": {
    "@apollo/client": "^3.10.4",
    "@babel/core": "^7.0.0-0",
    "@popperjs/core": "^2.11.0",
    "@rollup/plugin-graphql": "^2.0.3",
    "@sentry/browser": "^7.105.0",
    "@sentry/vue": "^7.105.0",
    "@tiptap/core": "^2.1.13",
    "@tiptap/extension-blockquote": "^2.3.0",
    "@tiptap/extension-code-block": "^2.3.0",
    "@tiptap/extension-color": "^2.1.13",
    "@tiptap/extension-heading": "^2.1.13",
    "@tiptap/extension-highlight": "^2.1.13",
    "@tiptap/extension-image": "^2.4.0",
    "@tiptap/extension-italic": "^2.4.0",
    "@tiptap/extension-link": "^2.3.0",
    "@tiptap/extension-paragraph": "^2.1.13",
    "@tiptap/extension-subscript": "^2.3.0",
    "@tiptap/extension-superscript": "^2.3.0",
    "@tiptap/extension-text-style": "^2.1.13",
    "@tiptap/extension-underline": "^2.1.13",
    "@tiptap/pm": "^2.1.13",
    "@tiptap/starter-kit": "^2.1.13",
    "@tiptap/vue-3": "^2.3.0",
    "@tweenjs/tween.js": "^19.0.0",
    "@types/lodash": "^4.14.136",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/apollo-composable": "^4.0.2",
    "@vue/apollo-option": "^4.0.0",
    "apollo-link-sentry": "^3.2.1",
    "apollo-upload-client": "^17.0.0",
    "axios": "^1.7.4",
    "cropperjs": "^1.6.1",
    "dayjs": "^1.10.7",
    "focus-visible": "^5.2.0",
    "formla": "^1.3.1",
    "fuse.js": "^6.0.0",
    "heic2any": "^0.0.3",
    "lodash": "^4.17.13",
    "pluralize": "^8.0.0",
    "prosemirror-state": "^1.4.3",
    "pusher-js": "^8.0.0",
    "react": "^18.2.0",
    "sortablejs": "^1.14.0",
    "vite": "^4.5.3",
    "vue": "^3.0.0",
    "vue-dompurify-html": "^4.0.0",
    "vue-i18n": "^9.1.6",
    "vue-router": "^4.0.0",
    "vuedraggable": "^4.0.1"
  },
  "devDependencies": {
    "@graphql-eslint/eslint-plugin": "^3.20.1",
    "@intlify/eslint-plugin-vue-i18n": "^2.0.0",
    "@rollup/pluginutils": "^5.0.4",
    "@sentry/vite-plugin": "^2.7.1",
    "@vue/eslint-config-airbnb": "^7.0.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.0.0",
    "eslint-plugin-es": "^4.1.0",
    "eslint-plugin-html": "^7.0.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-vue": "^9.0.0",
    "eslint-plugin-vuejs-accessibility": "^2.1.0",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.11.0",
    "htmlhint": "^1.1.4",
    "node-notifier": "^10.0.1",
    "postcss": "^8.4.5",
    "postcss-html": "^1.3.0",
    "postcss-import": "^15.0.0",
    "stylelint": "^15.0.0",
    "stylelint-codeframe-formatter": "^1.0.5",
    "stylelint-config-recommended-vue": "^1.1.0",
    "stylelint-no-unsupported-browser-features": "^6.0.0",
    "stylelint-order": "^6.0.0",
    "tailwindcss": "^3.0.15",
    "tape": "^5.4.1",
    "vite-plugin-resolve": "^2.4.1",
    "vite-plugin-stylelint": "^5.1.1",
    "vite-plugin-vue-devtools": "^7.0.14"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "git add"
    ],
    "*.{vue,htm,html,css,sss,less,scss}": [
      "git add"
    ]
  }
}
